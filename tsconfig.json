{
    "compilerOptions": {
       "target": "es6",
       "strict": true,
       "module": "commonjs",
       "moduleResolution": "node",
       "declaration": true,
       "sourceMap": true,
       "removeComments": true,
       "preserveConstEnums": true,
       "experimentalDecorators": true,
       "newLine": "LF"
    },
    "exclude": [
       "node_modules"
    ]
}
